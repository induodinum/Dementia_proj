    "def fluc(X,axis,rs,rf,diff=0.15):\n",
    "    cnt = 0\n",
    "    mult = 0\n",
    "    j = 1\n",
    "\n",
    "    i = rs\n",
    "    while(i+j<rf):\n",
    "        if(abs(X[i+j][axis]-X[i][axis])>diff):\n",
    "            if(cnt==0):\n",
    "                if(X[i+j][axis]>X[i][axis]):\n",
    "                    mult = 1\n",
    "                else:\n",
    "                    mult = -1\n",
    "                cnt += 1\n",
    "            else:\n",
    "                if((X[i+j][axis]>X[i][axis] and mult==-1) or \n",
    "                   (X[i+j][axis]<X[i][axis] and mult==1)):\n",
    "                    mult *= -1\n",
    "                    cnt += 1\n",
    "            i += j-1  \n",
    "        else:\n",
    "            if(cnt==0):\n",
    "                if(X[i+j][axis]>X[i][axis]):\n",
    "                    mult = 1\n",
    "                else:\n",
    "                    mult = -1\n",
    "        i += 1\n",
    "\n",
    "#     print(rf, cnt)\n",
    "    return cnt"
   ]
