    "def binarize_walk_prd(walk_prd, y):\n",
    "    walk_bin = np.zeros(len(y))\n",
    "    \n",
    "    for w in walk_prd:\n",
    "        for i in range(w.s, w.f+1):\n",
    "            walk_bin[i] = 1\n",
    "    \n",
    "    return walk_bin"
